{
  "name": "eslint-config-ryanditjia",
  "version": "3.0.0",
  "description": "ESLint config heavily inspired by create-react-app, with addition of prettier",
  "keywords": [
    "eslint",
    "eslintconfig",
    "prettier",
    "javascript",
    "react"
  ],
  "homepage": "https://github.com/ryanditjia/eslint-config-ryanditjia#readme",
  "bugs": "https://github.com/ryanditjia/eslint-config-ryanditjia/issues",
  "license": "MIT",
  "author": "Ryandi Tjia <ryandi.tjia@me.com> (https://artycurate.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanditjia/eslint-config-ryanditjia.git"
  },
  "scripts": {
    "format": "prettier --write \"*.js\" \"src/**/*.js\"",
    "lint": "eslint ./ --ext .js"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "confusing-browser-globals": "^1.0.5",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "0.0.0"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "prettier": "^1.15.3"
  },
  "peerDependencies": {
    "eslint": "^5.7.0",
    "prettier": "^1.14.3"
  }
}
