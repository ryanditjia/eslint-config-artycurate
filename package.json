{
  "name": "eslint-config-ryanditjia",
  "version": "2.1.0",
  "description": "ESLint config that extends Airbnb, Prettier as formatter, and optional config to play well with Gatsby and React development.",
  "keywords": [
    "eslint",
    "eslintconfig",
    "airbnb",
    "prettier",
    "javascript",
    "react",
    "gatsby"
  ],
  "homepage": "https://github.com/ryanditjia/eslint-config-ryanditjia#readme",
  "bugs": "https://github.com/ryanditjia/eslint-config-ryanditjia/issues",
  "license": "MIT",
  "author": "Ryandi Tjia <ryandi.tjia@me.com> (https://artycurate.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanditjia/eslint-config-ryanditjia.git"
  },
  "scripts": {
    "format": "prettier --write \"*.js\" \"src/**/*.js\"",
    "lint": "eslint ./ --ext .js"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "prettier": "^1.14.3"
  },
  "peerDependencies": {
    "eslint": "^5.2.0",
    "prettier": "^1.13.7"
  }
}
